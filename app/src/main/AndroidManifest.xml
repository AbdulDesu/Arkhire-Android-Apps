<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sizdev.arkhirefortalent">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ArkhireTalent">
        <activity android:name=".homepage.ResetPasswordActivity"></activity>
        <activity android:name=".homepage.item.DemoHiringProgress" />
        <activity android:name=".homepage.profile.NewTalentProfile" />
        <activity android:name=".homepage.profile.TalentProfileActivity" />
        <activity android:name=".administration.ForgetPassword" />
        <activity android:name=".administration.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".administration.RegisterActivity" />
        <activity android:name=".homepage.HomeActivity" />
    </application>

</manifest>