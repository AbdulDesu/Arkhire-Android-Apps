<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/arkhirenavigation"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.sizdev.arkhirefortalent.onboarding.SplashScreenFragment"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreenFragment_to_welcomeViewPager5"
            app:destination="@id/welcomeViewPager5"
            app:popUpTo="@id/splashScreenFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashScreenFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/splashScreenFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/welcomeViewPager5"
        android:name="com.sizdev.arkhirefortalent.onboarding.WelcomeViewPager"
        android:label="fragment_welcome_view_pager"
        tools:layout="@layout/fragment_welcome_view_pager" >
        <action
            android:id="@+id/action_welcomeViewPager5_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/welcomeViewPager5"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.sizdev.arkhirefortalent.administration.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" />
</navigation>